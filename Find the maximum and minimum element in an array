using System;

public class GFG{
	static public void Main (){
		int[] arr = {45,67,43,65,98,89,76,67};
		int min = 100,max=0;
		
		for(int i =0;i<arr.Length;i++)
		{
		    if(arr[i]>max)
		        max=arr[i];
		    if(arr[i]<min)
		        min=arr[i];
		        
		}
		Console.WriteLine("max = "+max+" min = "+min);
	}
}
