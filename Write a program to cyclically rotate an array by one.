using System;

public class GFG{
	static public void Main (){
		int[] arr = {1,2,3,4,5};
		int high = arr.Length;
		int temp = arr[high-1];
		int i ;
		for(i =high-1;i>0;i--)
		{
		    arr[i]=arr[i-1];
		    
		}
		arr[i]= temp;
		foreach(int elements in arr)
		{
		    Console.WriteLine(elements + " ");
		}
	}
}
