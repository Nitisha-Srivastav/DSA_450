using System;

public class GFG{
	static public void Main (){
		int[] array = {1,2,3,4,5,6,7,8};
		int len = array.Length;
		int j = len -1,i=0;
		while(i<j)
		{
		    int temp = array[i];
		    array[i]=array[j];
		    array[j]= temp;
		    j--;
		    i++;
		    
		}
		
		foreach(int elements in array)
		{
		    Console.Write(elements+" ");
		}
	}
}
